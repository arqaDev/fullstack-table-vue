{"ast":null,"code":"import axios from 'axios';\nimport TableComponent from './components/TableComponent.vue';\nimport ModalComponent from './components/UI/ModalComponent.vue';\nimport SearchAndSort from './components/UI/SearchAndSort.vue';\nexport default {\n  name: 'App',\n  components: {\n    TableComponent,\n    ModalComponent,\n    SearchAndSort\n  },\n\n  data() {\n    return {\n      isShowen: false,\n      selectedSort: ''\n    };\n  },\n\n  methods: {\n    addRow: async (title, quantity, distance) => {\n      const $host = axios.create({\n        baseURL: process.env.VUE_APP_API_URL\n      });\n      const data = await $host.post('api/table', {\n        title,\n        quantity,\n        distance\n      });\n      return data;\n    },\n\n    setSelectedSort(type) {\n      this.selectedSort = type;\n      console.log(this.selectedSort);\n    },\n\n    showModal() {\n      this.isShowen = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBE,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,cAAP,MAA2B,oCAA3B;AACA,OAAOC,aAAP,MAA0B,mCAA1B;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVJ,cADU;IAEVC,cAFU;IAGVC;EAHU,CAFC;;EAObG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KADL;MAELC,YAAY,EAAE;IAFT,CAAP;EAID,CAZY;;EAabC,OAAO,EAAE;IACPC,MAAM,EAAE,OAAOC,KAAP,EAAcC,QAAd,EAAwBC,QAAxB,KAAqC;MAC3C,MAAMC,KAAI,GAAId,KAAK,CAACe,MAAN,CAAa;QACvBC,OAAO,EAAEC,OAAO,CAACC,GAAR,CAAYC;MADE,CAAb,CAAd;MAGA,MAAMb,IAAG,GAAI,MAAMQ,KAAK,CAACM,IAAN,CAAW,WAAX,EAAwB;QAACT,KAAD;QAAQC,QAAR;QAAkBC;MAAlB,CAAxB,CAAnB;MACA,OAAOP,IAAP;IACD,CAPM;;IAQPe,eAAe,CAACC,IAAD,EAAO;MACpB,KAAKd,YAAL,GAAoBc,IAApB;MACAC,OAAO,CAACC,GAAR,CAAY,KAAKhB,YAAjB;IACD,CAXM;;IAYPiB,SAAS,GAAG;MACV,KAAKlB,QAAL,GAAgB,IAAhB;IACF;;EAdO;AAbI,CAAf","names":["axios","TableComponent","ModalComponent","SearchAndSort","name","components","data","isShowen","selectedSort","methods","addRow","title","quantity","distance","$host","create","baseURL","process","env","VUE_APP_API_URL","post","setSelectedSort","type","console","log","showModal"],"sourceRoot":"","sources":["C:\\Users\\Лидия\\Desktop\\Frontend\\fullstack-practice\\client\\src\\App.vue"],"sourcesContent":["<template>\n  <modal-component\n    :show=\"isShowen\"\n    @create=\"addRow\"\n    v-model:show=\"isShowen\"\n  />\n  <div style=\"display: flex; justify-content: space-between; align-items: center;\">\n    <custom-button @click=\"showModal\">Add Row</custom-button>\n    <search-and-sort\n      :modelValue=\"selectedSort\"\n      @update:modelValue=\"setSelectedSort\"\n    />\n  </div>\n  <table-component/> \n</template>\n\n<script>\n  import axios from 'axios';\n  import TableComponent from './components/TableComponent.vue';\n  import ModalComponent from './components/UI/ModalComponent.vue';\n  import SearchAndSort from './components/UI/SearchAndSort.vue';\n\n  export default {\n    name: 'App',\n    components: {\n      TableComponent,\n      ModalComponent,\n      SearchAndSort\n    },\n    data() {\n      return {\n        isShowen: false,\n        selectedSort: ''\n      }\n    },\n    methods: {\n      addRow: async (title, quantity, distance) => {\n        const $host = axios.create({\n            baseURL: process.env.VUE_APP_API_URL\n        })\n        const data = await $host.post('api/table', {title, quantity, distance})\n        return data\n      },\n      setSelectedSort(type) {\n        this.selectedSort = type\n        console.log(this.selectedSort)\n      },\n      showModal() {\n        this.isShowen = true\n      }\n  }\n}\n</script>\n\n<style>\n  @import url('https://fonts.googleapis.com/css2?family=Poppins&display=swap');\n\n  * {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n    font-size: 14px;\n    font-family: 'Poppins', sans-serif;\n  }\n\n  body {\n    padding: 5% 10%;\n    background: #f5f5f5;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}